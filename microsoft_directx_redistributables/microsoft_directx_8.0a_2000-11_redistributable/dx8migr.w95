[version]
Layoutfile=layout.inf, layout1.inf, layout2.inf
Signature="$CHICAGO$"
SetupClass=BASE

; add manufacturer section to include these files in the driver cabs
[Manufacturer]

[BaseWinOptions]
Directx.reg

[DestinationDirs]
file.cop = 11       ; LDID_SYS
vmmfile.cop = 11, vmm32
file.del = 11
gm.copy = 10,system32\drivers
gm.del = 11

[Directx.reg]
UpdateInis=Del.dir
CopyFiles=file.cop, vmmfile.cop, gm.copy
AddReg=add.reg
DelFiles=gm.del

[file.cop]

[vmmfile.cop]
vmcpd.vxd

[gm.copy]
gm.dls,gm16.dls
gameenum.sys
hidgame.sys

[gm.del]
gm16.dls

[file.del]
fchand.dll

[Del.dir]
wininit.ini,DIRNUL,,"%11%\dxbackup=1"

[add.reg]
HKLM,Software\Microsoft\DirectMusic,"GMFilePath",,"%10%\system32\drivers\gm.dls"

[DestinationDirs]
Del.dir      = 10		; LDID_WIN

